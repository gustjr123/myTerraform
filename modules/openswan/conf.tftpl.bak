conn Tunnel1
    authby=secret
    auto=start
    left=%defaultroute
    leftid=${var.leftid}
    right=${var.right1}
    type=tunnel
    ikelifetime=8h
    keylife=1h
    phase2alg=aes128-sha1;modp1024
    ike=aes128-sha1;modp1024
    #auth=esp
    keyingtries=%forever
    keyexchange=ike
    leftsubnet=${var.leftsubnet}
    rightsubnet=${var.rightsubnet1}
    dpddelay=10
    dpdtimeout=30
    dpdaction=restart_by_peer
    overlapip=yes

